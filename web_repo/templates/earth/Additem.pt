                            <option value="Class Project">Class Project</option>
                            <option value="Camp">ค่าย</option>
                            <option value="Competition">การแข่งขัน</option>
                            <option value="Normal Activity">กิจกรรมทั่วไป</option>
                        </select>
                    </div>
                </div>

            </div>
            <div class="form-group">
                <label class="col-md-2 control-label"></label>
                <div class="col-md-5 ">
